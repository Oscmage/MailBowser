<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.control.*?>

<HBox id="topbar" alignment="CENTER_LEFT" maxHeight="60.0" maxWidth="1.7976931348623157E308" prefHeight="60.0" spacing="10.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.chl.mailbowser.presenters.TopbarPresenter">
    <children>
      <HBox alignment="CENTER_LEFT" maxWidth="420.0" spacing="10.0">
         <children>
                <Button id="newEmailButton" maxHeight="40.0" maxWidth="-Infinity" minHeight="40.0" minWidth="-Infinity" mnemonicParsing="false" onAction="#newEmailButtonActionPerformed" prefHeight="40.0" prefWidth="180.0" text="New Email" />
                <TextField fx:id="searchField" layoutY="10.0" maxHeight="40.0" maxWidth="-Infinity" minHeight="40.0" minWidth="-Infinity" onAction="#searchFieldOnAction" prefHeight="40.0" prefWidth="220.0" promptText="Search here...">
                    <opaqueInsets>
                        <Insets />
                    </opaqueInsets>
               <HBox.margin>
                  <Insets left="10.0" right="10.0" />
               </HBox.margin>
                </TextField>
         </children>
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
         <HBox.margin>
            <Insets />
         </HBox.margin>
      </HBox>
          <Button maxHeight="40.0" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#refetchButtonOnAction" prefHeight="40.0" text="Refetch" />
      <HBox alignment="CENTER_RIGHT" layoutX="14.0" layoutY="10.0" maxWidth="1.7976931348623157E308" spacing="10.0" HBox.hgrow="ALWAYS">
         <children>
                <Button fx:id="forwardButton" maxHeight="40.0" maxWidth="1.7976931348623157E308" minHeight="40.0" mnemonicParsing="false" onAction="#forwardButtonOnAction" prefHeight="40.0" text="Forward" />
                <Button fx:id="replyButton" maxHeight="40.0" maxWidth="1.7976931348623157E308" minHeight="40.0" mnemonicParsing="false" onAction="#replyButtonOnAction" prefHeight="40.0" text="Reply" />
                <Button fx:id="replyAllButton" maxHeight="40.0" maxWidth="1.7976931348623157E308" minHeight="40.0" mnemonicParsing="false" onAction="#replyAllButtonOnAction" prefHeight="40.0" text="Reply All" />
                <TextField fx:id="addTagTextField" maxHeight="40.0" minHeight="40.0" onAction="#addTagOnAction" prefHeight="40.0" promptText="Add new tag..." />
         </children>
      </HBox>
    </children>
   <padding>
      <Insets left="10.0" right="10.0" />
   </padding>
</HBox>

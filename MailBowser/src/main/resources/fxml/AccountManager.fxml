<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.VBox ?>
<?import javafx.scene.layout.HBox?>

<fx:root type="javafx.scene.layout.VBox" stylesheets="@../css/style.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
<children>
   <HBox maxHeight="1.7976931348623157E308" styleClass="menuBar" VBox.vgrow="NEVER">
      <children>
         <HBox alignment="CENTER_LEFT" spacing="10.0" HBox.hgrow="ALWAYS">
            <children>
               <Button fx:id="addAccountButton" styleClass="circle, fontawesome" mnemonicParsing="false" onAction="#addAccountButtonOnAction" text="&#xf067;" />
               <Button fx:id="deleteAccountButton" styleClass="circle, fontawesome" mnemonicParsing="false" onAction="#deleteAccountButtonOnAction" text="&#xf00d;" />
            </children>
         </HBox>
         <HBox alignment="CENTER_RIGHT" spacing="10.0" HBox.hgrow="NEVER">
            <children>
               <Button fx:id="saveAccountButton" styleClass="circle, fontawesome" mnemonicParsing="false" onAction="#saveAccountButtonOnAction" text="&#xf0c7;" VBox.vgrow="NEVER" />
            </children>
         </HBox>
      </children>
   </HBox>
   <HBox alignment="CENTER" VBox.vgrow="ALWAYS">
      <children>
         <ListView fx:id="accountsList" prefWidth="160.0" HBox.hgrow="ALWAYS" />
         <ScrollPane fx:id="accountForm" hbarPolicy="NEVER" minWidth="240.0" maxWidth="240.0" prefViewportWidth="240.0" HBox.hgrow="ALWAYS" />
      </children>
   </HBox>
</children>
</fx:root>
